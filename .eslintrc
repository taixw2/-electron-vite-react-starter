{
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
